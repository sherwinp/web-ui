	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd">

	<initialize-in-order>false</initialize-in-order>
	<module>

		<ejb>lib/persist.jar</ejb>

	</module>

	<module>

		<web>

			<web-uri>web.war</web-uri>

